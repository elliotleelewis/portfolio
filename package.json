  "devDependencies": {
    "@astrojs/check": "^0.9.0",
    "@commitlint/cli": "^20.0.0",
    "@commitlint/config-conventional": "^20.0.0",
    "@eslint/compat": "^2.0.0",
    "@eslint/eslintrc": "^3.0.0",
    "@eslint/js": "^9.0.0",
    "@eslint-community/eslint-plugin-eslint-comments": "^4.0.0",
